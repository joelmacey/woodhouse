{
  "manifest_version": 2,
  "name": "Woodhouse Extension",
  "version": "0.1",
  "permissions": [
		"tabs",
		"https://*.amazonaws.com/"
	],
  "background" : {
    "persistent": true,
    "scripts": [
      "aws-sdk.js",
      "index.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["index.js", "aws-sdk.js"]
    }
  ]
}
