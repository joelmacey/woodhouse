{
  "manifest_version": 2,
  "name": "Woodhouse extension",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
  "version": "0.1",
  "permissions": [
		"tabs",
		"https://*.amazonaws.com/"
	],
  "background" : {
    "persistent": true,
    "scripts": [
      "aws-sdk.js",
      "index.js"
    ]
  }
}
